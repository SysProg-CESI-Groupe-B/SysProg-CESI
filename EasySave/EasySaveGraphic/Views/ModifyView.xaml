<Page x:Class="EasySaveGraphic.Views.ModifyView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EasySaveGraphic.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ModifyView">

    <Grid>
        <Button Content="Apply" HorizontalAlignment="Left" Margin="639,380,0,0" VerticalAlignment="Top" Click="Button_Apply_Click"/>
        <TextBox Name="NameTextBox" HorizontalAlignment="Left" Margin="110,183,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120"/>
        <ComboBox Name="TypeComboBox" HorizontalAlignment="Left" Margin="144,281,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="SourceTextBox" HorizontalAlignment="Left" Margin="400,120,0,0" TextWrapping="Wrap" Text="Source" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="DestTextBox" HorizontalAlignment="Left" Margin="377,183,0,0" TextWrapping="Wrap" Text="Dest" VerticalAlignment="Top" Width="120"/>
        <Button Content="Go Back" HorizontalAlignment="Left" Margin="144,412,0,0" VerticalAlignment="Top" Click="Button_GoBack_Click" Cursor="Hand"/>
        <Button Content="..." HorizontalAlignment="Left" Margin="437,380,0,0" VerticalAlignment="Top" Click="Button_Source_Click"/>
        <Button Content="..." HorizontalAlignment="Left" Margin="517,328,0,0" VerticalAlignment="Top" Click="Button_Dest_Click"/>
    </Grid>
</Page>
